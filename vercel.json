{
  "version": 2,
  "framework": null,
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/build",
  "installCommand": "cd frontend && npm install",
  "routes": [
    { "src": "/api/callback(.*)", "dest": "/api/auth.js" },
    { "src": "/api/auth(.*)", "dest": "/api/auth.js" },
    { "src": "/api/playback(.*)", "dest": "/api/playback.js" },
    { "src": "/api/search(.*)", "dest": "/api/search.js" },
    { "src": "/api/session(.*)", "dest": "/api/session.js" },
    { "src": "/api/sync(.*)", "dest": "/api/sync.js" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}