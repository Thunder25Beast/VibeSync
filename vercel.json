{
  "version": 2,
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/build",
  "functions": {
    "api/*.js": {
      "runtime": "@vercel/node@3"
    }
  },
  "rewrites": [
    { "source": "/api/callback", "destination": "/api/auth" },
    { "source": "/((?!api/).*)", "destination": "/index.html" }
  ]
}